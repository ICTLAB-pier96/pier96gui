<p>
  <strong>Title:</strong>
  <%= @image.title %>
</p>
 
<p>
  <strong>Base Image:</strong>
  <%= @image.base_image %>
</p>

<p>
  <strong>Created:</strong>
  <%= @image.date_added %>
</p>

<p>
  <strong>Dockerfile:</strong>
  <%= @image.filename %>
</p>

<%= form_tag({action: :upload}, multipart: true) do %>
  <%= file_field_tag 'file' %>
  <%= submit_tag 'file' %>
<% end %>



<%= link_to 'Back', images_path %>
