<h1 class="blue-text text-darken-3">Listing images <a href="/images.json">{JSON}</a></h1>
<%= link_to 'Add image', new_image_path, class: "blue darken-3 btn" %>
<br>
<table class="striped">
  <tr>
    <th>Id</th>
    <th>RepoTags</th>
    <th>Created</th>
    <th colspan="3"></th>
  </tr>
 
  <% @images.each do |image| %>
    <tr>
      <td><%= link_to "#{image.id}", image_path(:id => image.id) %></td>
      <td><%= image.repo %>/<%= image.image %></td>
      <td><%= image.created_at %></td>
      <td><%= link_to "Remove",image_destroy_path(id: image.id), class: "blue darken-3 btn" %></td>
  <% end %>
</table>